  "license": "ISC",
  "devDependencies": {
    "@protobuf-ts/plugin": "2.7.0",
    "@types/node": "18.19.26",
    "dotenv": "16.0.1",
    "eslint": "8.18.0",
    "nodemon": "2.0.18",
    "prettier": "2.7.1",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "kafkajs": "2.0.2"
  }
}
